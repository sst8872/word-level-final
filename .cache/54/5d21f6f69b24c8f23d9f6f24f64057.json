ne":200,"column":0},"generated":{"line":200,"column":0}},{"source":"css/style.css","original":{"line":201,"column":0},"generated":{"line":201,"column":0}},{"source":"css/style.css","original":{"line":202,"column":0},"generated":{"line":202,"column":0}},{"source":"css/style.css","original":{"line":203,"column":0},"generated":{"line":203,"column":0}},{"source":"css/style.css","original":{"line":204,"column":0},"generated":{"line":204,"column":0}},{"source":"css/style.css","original":{"line":205,"column":0},"generated":{"line":205,"column":0}},{"source":"css/style.css","original":{"line":206,"column":0},"generated":{"line":206,"column":0}}],"sources":{"css/style.css":"* {\r\n    margin: 0;\r\n    padding: 0;\r\n}\r\n\r\nbody {\r\n    box-sizing: border-box;\r\n    font-family: Helvetica, Arial, sans-serif;\r\n    margin:60px 40px 10px 40px;\r\n    overscroll-behavior: contain;\r\n    /* css gradient */\r\n    background: rgb(238,174,202);\r\n    background: radial-gradient(circle, rgba(238,174,202,1) 0%, rgba(148,187,233,1) 100%);\r\n}\r\n\r\n/* Style home buttons */\r\n\r\n.btn {\r\n    border: none; /* Remove borders */\r\n    color: #444; /* White text */\r\n    padding: 2px 1px; /* Some padding */\r\n    font-size: 40px; /* Set a font size */\r\n    cursor: pointer; /* Mouse pointer on hover */\r\n}\r\n.btn:hover {\r\n    color: #3b5998;\r\n}\r\n.home {\r\n    margin-top: 30px;\r\n    margin-right: 4px;\r\n}\r\n/* Darker background on mouse-over */\r\n\r\n.level-box {\r\n    display: flex;\r\n}\r\n.level-box > .day {\r\n    cursor: pointer;\r\n}\r\n\r\n#wrapper {\r\n    max-width: 768px;\r\n    margin: 20px auto 0;\r\n    height: 100%;\r\n    padding: 20px;\r\n    overflow-y: auto;\r\n}\r\n\r\n@media screen and (max-width: 800px) {\r\n    #wrapper {\r\n        height: 700px;\r\n    }\r\n}\r\n\r\n@media screen and (max-width: 330px) {\r\n    #wrapper {\r\n        height: 400px;\r\n    }\r\n}\r\n\r\n/* Hide scrollbar for Chrome, Safari and Opera */\r\n#wrapper::-webkit-scrollbar {\r\n    display: none;\r\n}\r\n\r\n/* Hide scrollbar for IE and Edge */\r\n#wrapper {\r\n    -ms-overflow-style: none;\r\n}\r\n\r\n/* game grid */\r\n.game {\r\n    position: relative;\r\n    width: 90%;\r\n    height: 100%;\r\n    margin: 0 auto;\r\n    /*margin-left: 20px;*/\r\n    letter-spacing: 1.5px;\r\n    display: grid;\r\n    grid-gap: 10px;\r\n    grid-template-columns: repeat(auto-fill, minmax(200px, 1fr));\r\n}\r\n\r\n@media screen and (max-width: 330px) {\r\n    .game {\r\n        grid-template-columns: repeat(auto-fill, minmax(180px, 1fr));\r\n        font-size: 0.9em;\r\n        text-align: center;\r\n    }\r\n}\r\n\r\n.box {\r\n    background-color: #444;\r\n    line-height: 1.4;\r\n    height: 30px;\r\n    color: #fff;\r\n    border-radius: 5px;\r\n    padding: 20px;\r\n    font-size: 150%;\r\n    text-align: center;\r\n    transition: all 0.2s ease-in-out;\r\n}\r\n\r\n\r\n/* start button */\r\n.start,\r\n.edit {\r\n    position: fixed;\r\n    top: 100px;\r\n    width: 180px;\r\n    background-color: transparent; /* blue */\r\n    border: 1px solid blue;\r\n    color: #444;\r\n    padding: 15px 15px;\r\n    text-align: center;\r\n    text-decoration: none;\r\n    display: inline-block;\r\n    font-size: 1.5em;\r\n    font-weight: lighter;\r\n    letter-spacing: 1.2px;\r\n    cursor: pointer;\r\n}\r\n.start:hover {\r\n    background-color: rgba(76,175,80,0.4); /* green */\r\n    color: white;\r\n}\r\n\r\n.edit {\r\n    top: 200px;\r\n}\r\n\r\n.edit:hover {\r\n    background-color: rgba(255,127,80,0.4);\r\n    color: white;\r\n}\r\n\r\n/* mode */\r\n.mode {\r\n    position: fixed;\r\n    left: 50px;\r\n    right: 50px;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    z-index: 10;\r\n}\r\n\r\n.mode > a {\r\n    font-size: 1.5em;\r\n    text-decoration: none;\r\n    display: inline-block;\r\n    padding: 8px 16px;\r\n    background: rgba(70, 112, 105, 0.6);\r\n    color: white;\r\n    border-radius: 5px;\r\n}\r\n\r\n\r\n/* index */\r\n.index {\r\n    display: flex;\r\n    position: fixed;\r\n    justify-content: space-between;\r\n    top: 70%;\r\n    left: 20px;\r\n    right: 20px;\r\n}\r\n\r\n\r\n\r\n.index > a {\r\n    font-size: 33px;\r\n    height: 100%;\r\n    line-height: 100%;\r\n    text-decoration: none;\r\n    display: inline-block;\r\n    padding: 4px;\r\n    color: #444;\r\n    cursor: pointer;\r\n}\r\n\r\n\r\n.index.hidden {\r\n    display: none;\r\n}\r\n\r\n/* tool tips */\r\n\r\n.tooltip-output {\r\n    z-index: 100;\r\n    display: none;\r\n    position: absolute;\r\n    border: 1px solid #ddd;\r\n    background-color: #444;\r\n    color: white;\r\n    padding: 5px;\r\n    top: 100px;\r\n    left: 50px;\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n"},"lineCount":206}},"error":null,"hash":"19c4668748e959b80234a01e4c6a7d2c","cacheData":{"env":{}}}